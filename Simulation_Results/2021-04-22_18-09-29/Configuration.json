    "brain": {
        "type": "LSTMNN",
        "hidden_layer_structure": [
            10
        ],
        "diagonal_hidden_to_hidden": true,
        "use_bias": true
    },
    "optimizer": {
        "type": "CMA-ES-Deap",
        "population_size": 112,
        "sigma": 1.0
    }
